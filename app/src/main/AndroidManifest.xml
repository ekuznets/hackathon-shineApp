<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eugene.testapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/heart_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".optionsActivity" />
        <activity
            android:name=".TakingTestActivity"
            android:label="@string/title_activity_taking_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LinksActivity" />
        <activity android:name=".aboutUsActivity"></activity>
    </application>

</manifest>